<div class="article-layout-container page-width-desktop">
  <aside class="content-sidebar article-layout">
    {% render 'content-hub-table-of-contents', block: section %}
  </aside>
  <div class="article-template__section-wrapper article-layout">
    {%- for block in section.blocks -%}
      {%- case block.type -%}
        {%- when '@app' -%}
        {% render block %}
        {%- when 'article_item' -%}
        {% render 'article-slider'
         , section_id: block.settings.article_section_id,
         headingLevel: block.settings.heading_level,
         heading: block.settings.heading,
         text_color: block.settings.text_color,
         show_image_overlay: block.settings.show_image_overlay,
         article_1: block.settings.article_1,
         article_2: block.settings.article_2,
         article_3: block.settings.article_3,
         article_4: block.settings.article_4,
         article_5: block.settings.article_5,
         article_6: block.settings.article_6 %}
        {% when 'content' %}
        {% render 'content-hub-content', block: block %}
        {%- when 'content_quote' -%}
        {% render 'content-hub-quote', block: block %}
        {%- when 'content_image' -%}
        {% render 'content-hub-image-block', block: block %}
        {%- when 'image_teaser' %}
        {% render 'content-hub-image-teaser', block: block %}
        {%- when 'summary_paragraph' -%}
        {% render 'content-hub-summary-paragraph', block: block %}
        {%- when 'content_hub_video' -%}
        {% render 'content-hub-video', block: block %}
        {%- when 'content_hub_html' -%}
        {% render 'content-hub-html', block: block %}
        {%- when 'collection_slideshow' -%}
        {% render 'content-hub-collection-slideshow', block: block %}
        {%- when 'product_slideshow' -%}
        {% render 'content-hub-product-slideshow', block: block %}
        {%- when 'content_hub_whitepaper' -%}
        {% render 'content-hub-whitepaper', block: block %}
        {%- when 'full_width_teaser' -%}
        {% render 'content-hub-full-width', block: block %}
        {%- when 'content_hub_summary' -%}
        {% render 'content-hub-summary', block: block %}
        {%- when 'round_image_teaser' -%}
        {% render 'content-hub-round-image-teaser', block: block %}
        {%- when 'content_steps' -%}
        {% render 'content-hub-content-steps', block: block %}
        {%- when 'faq_section' -%}
        {% render 'content-hub-faq', block: block %}
      {%- endcase -%}
    {%- endfor -%}
  </div>
</div>

{% if section.settings.table-of-contents-menu.title == blank %}
<style>
  .article-layout-container {
    justify-content: center !important;
  }
</style>
{% endif %}


